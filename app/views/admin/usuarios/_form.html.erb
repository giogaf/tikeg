
<%= form_for @user ,url: admin_usuarios_path, method: :post do  |f|%>
	<%= f.label :email %>
	<%= f.text_field :email %> <br>
	<%= f.label :password %>
	<%= f.text_field :password %> <br>
	<%= f.label :admin,'Administrador?' %>
	<%= f.check_box :admin %><br>
	<%= f.submit 'Crear usuario' %> 
<% end %>