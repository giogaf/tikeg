
<%= form_for @user , url: @user.id ? admin_usuario_path : admin_usuarios_path , 
		method: @user.id ? :patch : :post do  |f|%>

	<%= f.label :email %>
	<%= f.text_field :email %> <br>
	<%= f.label :password %>
	<%= f.text_field :password %> <br>
	<%= f.label :admin,'Administrador?' %>
	<%= f.check_box :admin %><br>
	<%= f.submit 'Crear usuario' %> 
<% end %>